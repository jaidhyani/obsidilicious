{
  "manifest_version": 2,
  "name": "Obsidilicious",
  "version": "1.0",
  "description": "Save links to Obsidian using the local REST API plugin.",
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "https://127.0.0.1:*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}